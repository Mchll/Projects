#include <stdio.h>
#include <stdlib.h>
#include <malloc.h>

typedef struct node {
    int data;
    struct node *next;
} node;

node* getLast(node *head) {
    while (head->next != NULL) {
        head = head->next;
    }
    return head;
}

void add(int val, node *head) {
    node *last = (node *) malloc(sizeof(node));
    if(last == NULL) {
        printf("ERROR\n");
    }
    node *tmp = (node *) malloc(sizeof(node));
    if(tmp == NULL) {
        printf("ERROR\n");
    }
    last = getLast(head);
    tmp->data = val;
    tmp->next = NULL;
    last->next = tmp;
}

void remove_from(int val, node *head) {
    node *tmp = (node *) malloc(sizeof(node));
    if(tmp == NULL) {
        printf("ERROR\n");
    }
    while(head->next != NULL) {
        if(head->next->data == val) {
            break;
        }
        head = head->next;
    }
    tmp = head->next;
    head->next = tmp->next;
    free(tmp);
}

void print(node *head) {
    head = head->next;
    while(head) {
        printf("%d ", head->data);
        head = head->next;
    }
    printf("\n");
}

void quit(node *head) {
    while(head) {
        node *tmp = head->next;
        free(head);
        head = tmp;
    }
}

void create_cycle(node *head) {
    node *last = (node *) malloc(sizeof(node));
    if(last == NULL) {
        printf("ERROR\n");
    }
    last = getLast(head);
    last->next = head->next;
}

int check_cycle(node *head) {
    node *fast = (node *) malloc(sizeof(node));
    if(fast == NULL) {
        printf("ERROR\n");
    }
    node *slow = (node *) malloc(sizeof(node));
    if(slow == NULL) {
        printf("ERROR\n");
    }
    fast = head->next;
    slow = head->next;
    if(fast == NULL) {
        return 0;
    }
    while(1) {
        fast = fast->next;
        if(fast == NULL) {
            return 0;
        }
        fast = fast->next;
        if(fast == NULL) {
            return 0;
        }
        slow = slow->next;
        if(fast == slow) {
            return 1;
        }
    }
}

int main()
{
    char command;
    int val = 0;
    node *head = (node *) malloc(sizeof(node));
    if(head == NULL) {
        printf("ERROR");
    }
    head->data = -111222;
    head->next = NULL;

    while(1) {

        scanf("%c", &command);

        if(command == 'a') {
            scanf("%d", &val);
            add(val, head);
        }

        if(command == 'r') {
            scanf("%d", &val);
            remove_from(val, head);
        }

        if(command == 'p') {
            print(head);
        }

        if(command == 'q') {
            quit(head);
            break;
        }

        if(command == 'c') {
            create_cycle(head);
            int cycle = -1;
            cycle = check_cycle(head);
            if(cycle == 1) {
                printf("CYCLE WAS FOUND\n");
                return 0;
            }
            else {
                printf("There are no cycles here\n");
            }
        }

    }
    return 0;
}
