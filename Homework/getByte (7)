#include <stdio.h>
#include <stdlib.h>
int getByte(int x, int n) {
    int mask = 0x000000ff;
    n = n+n;
    n = n+n;
    n = n+n;
    x = (x>>n)&mask;
    return x;
}
int main()
{
    int n = 0, x = 0, ans = 0;
    scanf("%x%d", &x, &n);
    ans = getByte(x, n);
    printf("%x", ans);
    return 0;
}
