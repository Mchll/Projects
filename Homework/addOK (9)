#include <stdio.h>
#include <stdlib.h>
int addOK(int x, int y) {
    int z = 0;
    z = x + y;
    x = (x >> 31) & 1;
    y = (y >> 31) & 1;
    z = (z >> 31) & 1;
    return (((z & x) + (z & y)) & 1 + ! (x | y | z));
}
int main()
{
    int x = 0, y = 0, ans = 0;
    scanf("%d%d", &x, &y);
    ans = addOK(x, y);
    printf("%d", ans);
    return 0;
}

