//11. Числа Фибоначчи.
#include <iostream>
#define ll long long
using namespace std;
ll n, fibfirst, fibsecond, fibthird;
int main()
{
	cin >> n;
	if(n == 1) {
		cout << 1;
		return 0;
	}
	if(n == 2) {
		cout << 2;
		return 0;
	}
	n = n - 2;
	fibsecond = 1;
	fibthird = 2;
	while(n > 0) {
		n--;
		fibfirst = fibsecond;
		fibsecond = fibthird;
		fibthird = fibfirst+fibsecond;
	}
	cout << fibthird;
	return 0;
}
