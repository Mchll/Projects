//5. Является ли массив симметричным.
#include <iostream>
#define ll long long
using namespace std;
ll a[100000], i, k = 0, n;
int main()
{
	cin >> n;
	for(i = 1; i <= n; ++i) {
		cin >> a[i];
	}
	for(i = 1; i <= n / 2; ++i) {
		if(a[i] != a[n - i + 1]) {
			k = 1;
		}
	}
	if(k == 1) {
		cout << "NO";
	}
	else {
		cout << "YES";
	}
	return 0;
}
