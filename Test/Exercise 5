//5. Является ли массив симметричным.
#include <iostream>
#define ll long long
using namespace std;
ll arr[100000], i, n;
bool ans = true;
int main()
{
	cin >> n;
	for(i = 1; i <= n; ++i) {
		cin >> arr[i];
	}
	for(i = 1; i <= n / 2; ++i) {
		if(arr[i] != arr[n - i + 1]) {
			ans = false;
		}
	}
	if(ans == false) {
		cout << "NO";
	}
	else {
		cout << "YES";
	}
	return 0;
}
