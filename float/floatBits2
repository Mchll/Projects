#include <stdio.h>
#include <stdlib.h>

union data {
    struct {
        unsigned int m : 23;
        unsigned int e : 8;
        unsigned int s : 1;
    } field;
    float f;
};

struct answer {
    unsigned int s;
    unsigned int m;
    unsigned int e;
};

void print_mantiss (int m) {
    int i = 0;
    for(i = 22; i >= 0; --i) {
        printf("%d", (m >> i) & 1);
    }
}

void print (struct answer ans) {
    if(ans.e  == 255) {
        if(ans.s == 0 && ans.m == 0) {
            printf("+inf");
        }
        else if(ans.s % 2 == 1 && ans.m == 0) {
            printf("-inf");
        }
        else printf("NaN");
    }
    else {
        printf("(-1)^(%d) * 1.", ans.s);
        print_mantiss(ans.m);
        printf(" * 2^(%d - 127)\n", ans.e);
    }
}

void floatBits2 (float f) {
    union data fl;
    fl.f = f;
    struct answer ans;
    ans.s = fl.field.s;
    ans.e = fl.field.e;
    ans.m = fl.field.m;
    print(ans);
}

int main()
{
    float f = 0;
    int f1 = 0, f2 = 0;
    scanf("%d", &f1);
    scanf("%d", &f2);
    f = ((double)(f1) / f2);
    floatBits2(f);

    return 0;
}
