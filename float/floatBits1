#include <stdio.h>
#include <stdlib.h>
union data {
    float f;
    int i;
};
struct answer{
    int s;
    int e;
    int m;
};

void print_mantiss (int m) {
    int i = 0;
    for(i = 22; i >= 0; --i) {
        printf("%d", (m >> i) & 1);
    }
}

void print (struct answer ans) {
    if(ans.e  == 255) {
        if(ans.s == 0 && ans.m == 0) {
            printf("+inf");
        }
        else if(ans.s % 2 == 1 && ans.m == 0) {
            printf("-inf");
        }
        else printf("NaN");
    }
    else {
        printf("(-1)^(%d) * 1.", ans.s);
        print_mantiss(ans.m);
        printf(" * 2^(%d - 127)\n", ans.e);
    }
}

void floatBits1 (float f) {
    union data fl;
    int mask_sign = 0x00000001, mask_exponent = 0x000000FF, mask_mantiss = 0x007FFFFF;
    struct answer ans;
    fl.f = f;
    ans.s = (fl.i >> 31) & mask_sign;
    ans.e = (fl.i >> 23) & mask_exponent;
    ans.m = (fl.i & mask_mantiss);
    print(ans);
}

int main()
{
    float f = 0;
    int f1 = 0, f2 = 0;
    scanf("%d", &f1);
    scanf("%d", &f2);
    f = ((double)(f1) / f2);
    floatBits1(f);
    return 0;
}
