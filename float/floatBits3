#include <stdio.h>
#include <stdlib.h>

struct answer {
    unsigned int s;
    unsigned int m;
    unsigned int e;
};

void print_mantiss (int m) {
    int i = 0;
    for(i = 22; i >= 0; --i) {
        printf("%d", (m >> i) & 1);
    }
}

void print (struct answer ans) {
    if(ans.e  == 255) {
        if(ans.s == 0 && ans.m == 0) {
            printf("+inf");
        }
        else if(ans.s % 2 == 1 && ans.m == 0) {
            printf("-inf");
        }
        else printf("NaN");
    }
    else {
        printf("(-1)^(%d) * 1.", ans.s);
        print_mantiss(ans.m);
        printf(" * 2^(%d - 127)\n", ans.e);
    }
}

void floatBits3 (float f) {
    int mask_sign = 0x00000001, mask_exponent = 0x000000FF, mask_mantiss = 0x007FFFFF;
    int *pointer = (int *) (&f);
    struct answer ans;
    ans.s = (*pointer >> 31) & mask_sign;
    ans.e = (*pointer >> 23) & mask_exponent;
    ans.m = (*pointer & mask_mantiss);
    print(ans);
}

int main()
{
    float f = 0;
    int f1 = 0, f2 = 0;
    scanf("%d", &f1);
    scanf("%d", &f2);
    f = ((double)(f1) / f2);
    floatBits3(f);

    return 0;
}
